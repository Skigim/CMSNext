{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "npm: dev",
            "type": "npm",
            "script": "dev",
            "isBackground": true,
            "problemMatcher": {
                "owner": "vite",
                "pattern": {
                    "regexp": "."
                },
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": "VITE.*ready in",
                    "endsPattern": "(Local|Network):.*http"
                }
            },
            "presentation": {
                "reveal": "always",
                "panel": "new"
            },
            "group": {
                "kind": "build",
                "isDefault": false
            }
        },
        {
            "label": "npm: build",
            "type": "npm",
            "script": "build",
            "problemMatcher": [],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "npm: test",
            "type": "npm",
            "script": "test",
            "problemMatcher": [],
            "group": {
                "kind": "test",
                "isDefault": true
            }
        },
        {
            "label": "npm: test:watch",
            "type": "npm",
            "script": "test:watch",
            "problemMatcher": [],
            "isBackground": true,
            "group": {
                "kind": "test",
                "isDefault": false
            }
        },
        {
            "label": "npm: lint",
            "type": "npm",
            "script": "lint",
            "problemMatcher": [
                "$eslint-stylish"
            ],
            "group": "none"
        }
    ]
}
