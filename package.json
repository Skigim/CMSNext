{
  "name": "case-tracking-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:pages": "tsc && vite build --base=/CMSNext/",
    "build:no-lint": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 200",
    "lint:check": "eslint . --ext ts,tsx --max-warnings 0",
    "preview": "vite preview",
    "test": "node --max-old-space-size=8192 ./node_modules/vitest/vitest.mjs run",
    "test:ui": "node --max-old-space-size=8192 ./node_modules/vitest/vitest.mjs --ui",
    "test:run": "node --max-old-space-size=8192 ./node_modules/vitest/vitest.mjs run",
    "test:coverage": "node --max-old-space-size=8192 ./node_modules/vitest/vitest.mjs run --coverage",
    "analyze": "tsc && ANALYZE=true vite build --mode analyze",
    "perf:baseline": "tsx scripts/performanceBaseline.ts",
    "test:watch": "node --max-old-space-size=8192 ./node_modules/vitest/vitest.mjs --watch",
    "seed": "npx tsx scripts/seedCli.ts",
    "seed:demo": "npx tsx scripts/seedCli.ts --preset demo",
    "seed:small": "npx tsx scripts/seedCli.ts --preset small",
    "seed:large": "npx tsx scripts/seedCli.ts --preset large",
    "usage:report": "tsx scripts/usageReport.ts",
    "bench:autosave": "tsx scripts/autosaveBenchmark.ts",
    "bench:dashboard": "tsx scripts/dashboardLoadBenchmark.ts",
    "analyze:nav": "tsx scripts/analyzeNavigationTrace.ts",
    "analyze:flamegraph": "tsx scripts/generateFlamegraph.ts",
    "dead-code": "knip --exclude duplicates",
    "dead-code:fix": "knip --fix --exclude duplicates",
    "dead-code:deps": "knip --dependencies",
    "dead-code:all": "knip"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/uuid": "^9.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "fuse.js": "^7.0.0",
    "lucide-react": "^0.544.0",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "^18.2.0",
    "react-is": "^19.2.3",
    "react-masonry-css": "^1.0.16",
    "recharts": "^3.2.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.9.1",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.0.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.0.0",
    "knip": "^5.82.1",
    "postcss": "^8.5.6",
    "puppeteer": "^24.26.1",
    "rollup-plugin-visualizer": "^5.13.0",
    "tailwindcss": "^4.1.13",
    "terser": "^5.44.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.6",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.2.4"
  }
}
